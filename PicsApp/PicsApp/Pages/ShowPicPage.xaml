<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PicsApp.Pages.ShowPicPage">
    <ContentPage.Content>
        <Grid RowDefinitions="90*, 10*">
            <Image x:Name="pic" Aspect="AspectFit" Grid.Row="0" />

            <Label x:Name="picL" FontSize="15" Grid.Row="1"  VerticalOptions="Center" />
        </Grid>
    </ContentPage.Content>
</ContentPage>